\txisetlanguage{Chinese}{2}{3}

\plainnonfrenchspacing

\gdef\putwordAppendix{附录}
\gdef\putwordChapter{章}
\gdef\putworderror{错误}
\gdef\putwordfile{文件}
\gdef\putwordin{在}
\gdef\putwordIndexIsEmpty{(空索引)}
\gdef\putwordIndexNonexistent{(索引不存在)}
\gdef\putwordInfo{信息}
\gdef\putwordInstanceVariableof{Instance Variable of}
\gdef\putwordMethodon{Method on}
\gdef\putwordNoTitle{无标题}
\gdef\putwordof{of}
\gdef\putwordon{on}
\gdef\putwordpage{页}
\gdef\putwordsection{section}
\gdef\putwordSection{节}
\gdef\putwordsee{查看}
\gdef\putwordSee{看}
\gdef\putwordShortTOC{简短目录}
\gdef\putwordTOC{目录}
%
\gdef\putwordMJan{一月}
\gdef\putwordMFeb{二月}
\gdef\putwordMMar{三月}
\gdef\putwordMApr{四月}
\gdef\putwordMMay{五月}
\gdef\putwordMJun{六月}
\gdef\putwordMJul{七月}
\gdef\putwordMAug{八月}
\gdef\putwordMSep{九月}
\gdef\putwordMOct{十月}
\gdef\putwordMNov{十一月}
\gdef\putwordMDec{十二月}
%
\gdef\putwordDefmac{宏}
\gdef\putwordDefspec{特殊形式}
\gdef\putwordDefvar{变量}
\gdef\putwordDefopt{用户选项}
\gdef\putwordDeffunc{功能}

